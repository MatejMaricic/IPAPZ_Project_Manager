<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
            <link href="{{ absolute_url(asset('lib/font-awesome/css/font-awesome.css')) }}" rel="stylesheet" />
            <link rel="stylesheet" type="text/css" href="{{ absolute_url(asset('css/zabuto_calendar.css')) }}">
            <link rel="stylesheet" type="text/css" href="{{ absolute_url(asset('lib/gritter/css/jquery.gritter.css')) }}" />
            <link href="{{ absolute_url(asset('css/style.css')) }}" rel="stylesheet">
            <link href="{{ absolute_url(asset('css/style-responsive.css')) }}" rel="stylesheet">



        {% endblock %}
    </head>
    <body>
    <div class="container">
        {% for message in app.flashes('success') %}
                        {{ message }}
        {% endfor %}

    </div>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
            <script src="{{ absolute_url(asset('lib/chart-master/Chart.js')) }}"></script>
            <script class="include" type="text/javascript" src="{{ absolute_url(asset('lib/jquery.dcjqaccordion.2.7.js')) }}"></script>
            <script src="{{ absolute_url(asset('lib/jquery.scrollTo.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('lib/jquery.nicescroll.js')) }}" type="text/javascript"></script>
            <script src="{{ absolute_url(asset('lib/jquery.sparkline.js')) }}"></script>
            <script src="{{ absolute_url(asset('lib/common-scripts.js')) }}"></script>
            <script type="text/javascript" src="{{ absolute_url(asset('lib/gritter/js/jquery.gritter.js')) }}"></script>
            <script type="text/javascript" src="{{ absolute_url(asset('lib/gritter-conf.js')) }}"></script>
            <script src="{{ absolute_url(asset('lib/sparkline-chart.js')) }}"></script>
            <script src="{{ absolute_url(asset('lib/zabuto_calendar.js')) }}"></script>

            <script src="{{ absolute_url(asset('lib/custom.js')) }}"></script>
        {% endblock %}
    </body>

</html>
